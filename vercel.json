{
  "version": 2,
  "builds": [
    {
      "src": "www.alexaglobalfinance.com/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/www.alexaglobalfinance.com/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/www.alexaglobalfinance.com/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/www.alexaglobalfinance.com/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/www.alexaglobalfinance.com/$1"
    }
  ]
}
